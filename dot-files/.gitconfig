[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Andrew Tu
	email = drewtu2@yahoo.com
[core]
	editor = vim
[push]
    default = matching
[status]
    submodulesummary = 1

# Add colors always!
[color]
      ui = auto
[color "branch"]
      current = yellow reverse
        local = yellow
          remote = green
[color "diff"]
      meta = yellow bold
        frag = magenta bold
          old = red bold
            new = green bold
[color "status"]
      added = yellow
        changed = green
          untracked = cyan
